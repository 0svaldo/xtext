module GenerateSpec

import org.eclipse.emf.mwe.utils.*

var targetDir = "xdoc-gen"
var modelPath = "xdoc/"

Workflow {
	component = @workflow.XDocGenerator { 
		// or define search scope explicitly
		modelPath = modelPath
		targetDir = targetDir
		document = "Xtext Documentation"
		pdfLatex = "/usr/texbin/pdflatex"
	}

}
