module GenerateSpec

import org.eclipse.emf.mwe.utils.*

var targetDir = "manual"
var modelPath = "xdoc"

Workflow {
	bean = workflow.Headless {}
	component = @workflow.XDocGenerator { 
		// or define search scope explicitly
		modelPath = modelPath
		targetDir = targetDir
		document = "Xtext 2.5 Documentation"
		pdfLatex = "/usr/texbin/pdflatex"
		release = true
//		cleanAfterRun = false
	}

}
