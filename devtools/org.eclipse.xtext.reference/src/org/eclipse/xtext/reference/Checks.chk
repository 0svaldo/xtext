import ReferenceGrammar;
/*
 * this check file is used to specify constraints for the derived metamodels
 * example:
 * context Entity WARNING "Entity names should start with a capital" :
 *    name.toFirstUpper() == name;
 */
context Spielplatz#groesse ERROR "Der Spielplatz ist zu gross fuer die Kinder." : 
	groesse < 100;
	
	
context Spielplatz#beschreibung WARNING "Der Spielplatz sollte eine Beschreibung haben." : 
	beschreibung!=null&&beschreibung!="";