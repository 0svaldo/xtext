<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>
	<extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.xtext.reference.ui.editor.ReferenceGrammarEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="xtest,tst"
            id="org.eclipse.xtext.reference.ReferenceGrammar"
            name="ReferenceGrammar Editor">
      </editor>
   </extension>
 <extension
         point="org.eclipse.ui.preferencePages">
		<page
			class="org.eclipse.xtext.ui.core.util.ServiceInjectingExecutableExtensionFactory:org.eclipse.xtext.ui.core.editor.preferences.LanguageRootPreferencePage/org.eclipse.xtext.reference.ReferenceGrammar"
			id="org.eclipse.xtext.reference.ReferenceGrammar"
			name="ReferenceGrammar">
			<keywordReference
				id="org.eclipse.xtext.reference.ui.ui.keyword_ReferenceGrammar">
			</keywordReference>
		</page>
		<page
		    category="org.eclipse.xtext.reference.ReferenceGrammar"
		    class="org.eclipse.xtext.ui.core.util.ServiceInjectingExecutableExtensionFactory:org.eclipse.xtext.ui.core.editor.preferences.EditorPreferencePage/org.eclipse.xtext.reference.ReferenceGrammar"
		    id="org.eclipse.xtext.reference.ReferenceGrammar.editor"
		    name="Editor">
		    	<keywordReference 
           			id="org.eclipse.xtext.keyword.editor">
     			</keywordReference>
		</page>
      </extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="org.eclipse.xtext.reference.ui.ui.keyword_ReferenceGrammar"
			label="ReferenceGrammar">
		</keyword>
		<keyword
            id="org.eclipse.xtext.reference.ReferenceGrammar.tasktags"
            label="Task Tags">
        </keyword>
	</extension>
	
   <extension
     point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.eclipse.xtext.reference.services.ReferenceGrammarResourceFactory"
            type="xtest">
      </parser>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.eclipse.xtext.reference.services.ReferenceGrammarResourceFactory"
            type="tst">
      </parser>
   </extension>
</plugin>
