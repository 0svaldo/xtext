<workflow>
	<property name="grammarURI" value="classpath:/org/eclipse/xtext/builtin/XtextBuiltin.xtext"/>
	<property name="runtimeProject" value="../org.eclipse.xtext"/>
	
	<component class="org.eclipse.xtext.generator.Generator">
		<grammarURI value="${grammarURI}"/>
		<pathRtProject value="${runtimeProject}"/>
		<fragments file="org/eclipse/xtext/generator/standardRuntimeFragments.mwe">
			<file.extensions value="NO_EXTENSIONS"/>
		</fragments>
	</component>
</workflow>