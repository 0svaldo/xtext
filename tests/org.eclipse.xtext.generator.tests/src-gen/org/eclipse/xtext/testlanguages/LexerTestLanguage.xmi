      </abstractTokens>
      <abstractTokens xsi:type="xtext:Assignment" feature="h" operator="=">
        <terminal xsi:type="xtext:RuleCall" rule="//@rules.2"/>
      </abstractTokens>
    </alternatives>
  </rules>
  <rules xsi:type="xtext:LexerRule" name="STRING" body=" '#' ('B')+ ">
    <type>
      <metamodel href="classpath:/org/eclipse/xtext/builtin/XtextBuiltin.xtext#//@metamodelDeclarations.0"/>
      <type xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </type>
