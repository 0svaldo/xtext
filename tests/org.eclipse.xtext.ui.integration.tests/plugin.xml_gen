            id="org.eclipse.xtext.ui.integration.tests.editor.bracketmatching.BmTestLanguage.templates"
            name="Templates">
            <keywordReference id="org.eclipse.xtext.ui.integration.tests.editor.bracketmatching.ui.keyword_BmTestLanguage"/>
        </page>
    </extension>
    <extension
        point="org.eclipse.ui.keywords">
        <keyword
            id="org.eclipse.xtext.ui.integration.tests.editor.bracketmatching.ui.keyword_BmTestLanguage"
            label="BmTestLanguage"/>
    </extension>
    <extension
